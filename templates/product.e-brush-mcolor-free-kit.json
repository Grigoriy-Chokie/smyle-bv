        "faq_item_pqGUpe": {
          "type": "faq_item",
          "settings": {
            "question": "<p>FAST & FREE SHIPPING</p>",
            "answer": "<p>Your ultimate cleaning experience is coming! And who likes shipping costs? No one! We deliver your package within 3-5 business days anywhere in Europe.</p>"
